{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "order",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "kode": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "jenis_marketing": {
      "type": "enumeration",
      "enum": [
        "online",
        "offline"
      ],
      "required": true
    },
    "jenis_pembayaran": {
      "type": "enumeration",
      "enum": [
        "cash",
        "kredit"
      ],
      "required": true
    },
    "tambahan": {
      "type": "string"
    },
    "diskon": {
      "type": "float"
    },
    "keterangan": {
      "type": "string"
    },
    "bonus": {
      "type": "string"
    },
    "tanggal": {
      "type": "date",
      "required": true
    },
    "customer_id": {
      "via": "order",
      "collection": "customer"
    },
    "branch_id": {
      "model": "branch",
      "via": "orders"
    },
    "users_permissions_user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "orders"
    },
    "closer": {
      "via": "orders",
      "plugin": "users-permissions",
      "model": "user"
    },
    "package": {
      "model": "package",
      "via": "order"
    },
    "address_id": {
      "model": "address"
    },
    "order_confirmation": {
      "via": "order_id",
      "model": "order-confirmation"
    }
  }
}
