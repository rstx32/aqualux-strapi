{
  "kind": "collectionType",
  "collectionName": "packages",
  "info": {
    "name": "package"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "nama": {
      "type": "string",
      "required": true
    },
    "harga": {
      "type": "integer",
      "required": true
    },
    "order": {
      "via": "package",
      "model": "order"
    }
  }
}
